<p id="notice"><%= notice %></p>

<h1>Solutions</h1>
<div class="container">
<table class="table table-striped table-hover">
  <thead>
    <tr>
      <th>Filename</th>
      <th>Content type</th>
      <th>Problem Id</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @solutions.each do |solution| %>
      <tr>
        <% if current_user == solution.user %>
         <td><%= solution.filename %></td>
         <td><%= solution.content_type %></td>
         <td><%= solution.problem_id %></td>
        <% end %>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

</div>
